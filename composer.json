{
    "name": "charles/medicore",
    "description": "MediCore - Hospital Management System",
    "type": "project",
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^3.0",
        "laravel/breeze": "1.9.2",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.15",
        "laravel/tinker": "^2.7",
        "maatwebsite/excel": "^3.1",
        "nesbot/carbon": "^2.73",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-permission": "^5.5",
        "carbonphp/carbon": "^2.73"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
